Model RaidOption {
	name: 'Raid Option'
	properties: [
		controller {
			name: 'Controller'
			type: Reference
			referenceTo: #RaidController
			required : true
		},
		raidType {
			name: 'Type'
			type: Reference
			referenceTo: #RaidType
			required : true
		},
		cost {
			name: 'Cost'
			type: Amount
		}
	]

}